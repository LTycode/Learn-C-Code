#include<stdio.h>
#include<assert.h>
char *my_strcpy(char* dest,const char* src)
{
	assert(dest != NULL);
	assert(src != NULL);
	char* ret = dest;
	//拷贝src指向的字符串到dest的空间，包括'\0'
	while (*dest++ = *src++)//*str!='\0'
	{
		//*dest = *src;
		//dest++;
		//src++;
		;
	}
	//*dest=*src;
	return ret;
}
int main()
{
	char arr1[] = "adfvcxgdf";
	char arr2[] = "hello";
	my_strcpy(arr1, arr2);
	printf("%s", arr1);
	return 0;
}
